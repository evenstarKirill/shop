dmin.tsx",["168","169","170","171"],[],"import { observer } from 'mobx-react-lite';\nimport React, { useContext, useEffect, useRef, useState } from 'react';\nimport {\n  Container,\n  Row,\n  Col,\n  Form,\n  Button,\n  Overlay,\n  Tooltip,\n} from 'react-bootstrap';\n\nimport { Context } from '..';\nimport { getTypes, getDevice, getBrands } from '../http/deviceApi';\n\nimport AdminBrandBar from '../components/Admin/AdminBrandBar/AdminBrandBar';\nimport CreateButton from '../components/Admin/CreateButton/CreateButton';\nimport AdminDeviceList from '../components/Admin/AdminDeviceList/AdminDeviceList';\nimport CreateDeviceModal from '../components/Admin/Modals/CreateModals/CreateDeviceModal/CreateDeviceModal';\nimport CreateBrandModal from '../components/Admin/Modals/CreateModals/CreateBrandModal/CreateBrandModal';\nimport CreateTypeModal from '../components/Admin/Modals/CreateModals/CreateTypeModal/CreateTypeModal';\nimport AdminTypeBar from '../components/Admin/AdminTypeBar/AdminTypeBar';\nimport EditBrandOrTypeModal from '../components/Admin/Modals/EditModals/EditBrandOrTypeModal/EditBrandOrTypeModal';\nimport EditDeviceModal from '../components/Admin/Modals/EditModals/EditDeviceModal/EditDeviceModal';\n\ninterface IModalSHow {\n  device: boolean;\n  type: boolean;\n  brand: boolean;\n  editDevice: boolean;\n  editType: boolean;\n  editBrand: boolean;\n}\n\nenum Modals {\n  type = 'type',\n  device = 'device',\n  brand = 'brand',\n  editType = 'editType',\n  editDevice = 'editDevice',\n  editBrand = 'editBrand',\n}\n\nconst Admin = observer(() => {\n  const { device } = useContext(Context);\n\n  const [show, setShow] = useState<IModalSHow>({\n    device: false,\n    type: false,\n    brand: false,\n    editDevice: false,\n    editType: false,\n    editBrand: false,\n  });\n\n  const handleShow = (modal: Modals) =>\n    setShow((state: IModalSHow) => ({ ...state, [modal]: !state[modal] }));\n\n  useEffect(() => {\n    getTypes().then((data) => device.setTypes(data));\n    getDevice().then((data) => {\n      device.setDevices(data);\n    });\n    getBrands().then((data) => device.setBrands(data));\n  }, []);\n\n  //TODO: button \"show all\" - separate component\n\n  return (\n    <Container>\n      <Row className=\"mt-3\">\n        <Col md={3}>\n          <Form.Group className=\"mb-3\" controlId=\"formBasicPassword\">\n            <CreateButton\n              name={Modals.type}\n              handleShow={() => handleShow(Modals.type)}\n            />\n            <Form.Label>Types</Form.Label>\n          </Form.Group>\n          <AdminTypeBar handleShow={() => handleShow(Modals.editType)} />\n          <div className=\"mt-5\">\n            <Form.Group className=\"mb-3\" controlId=\"formBasicPassword\">\n              <CreateButton\n                name={Modals.brand}\n                handleShow={() => handleShow(Modals.brand)}\n              />\n              <Form.Label>Brands</Form.Label>\n            </Form.Group>\n            <AdminBrandBar handleShow={() => handleShow(Modals.editBrand)} />\n          </div>\n        </Col>\n        <Col md={9}>\n          <CreateButton\n            name={Modals.device}\n            handleShow={() => handleShow(Modals.device)}\n          />\n          <AdminDeviceList handleShow={() => handleShow(Modals.editDevice)} />\n        </Col>\n        {show.device && (\n          <CreateDeviceModal\n            show={show.device}\n            handleShow={() => handleShow(Modals.device)}\n          />\n        )}\n        {show.brand && (\n          <CreateBrandModal\n            show={show.brand}\n            handleShow={() => handleShow(Modals.brand)}\n          />\n        )}\n        {show.type && (\n          <CreateTypeModal\n            show={show.type}\n            handleShow={() => handleShow(Modals.type)}\n          />\n        )}\n        {show.editType && (\n          <EditBrandOrTypeModal\n            name={Modals.type}\n            show={show.editType}\n            handleShow={() => handleShow(Modals.editType)}\n          />\n        )}\n        {show.editBrand && (\n          <EditBrandOrTypeModal\n            name={Modals.brand}\n            show={show.editBrand}\n            handleShow={() => handleShow(Modals.editBrand)}\n          />\n        )}\n        {show.editDevice && (\n          <EditDeviceModal\n            show={show.editDevice}\n            handleShow={() => handleShow(Modals.editDevice)}\n          />\n        )}\n      </Row>\n    </Container>\n  );\n});\n\nexport default Admin;\n","/Users/apple/Documents/work/2022/fullstack-shop/client/src/pages/auth.tsx",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/pages/device.tsx",["172","173"],[],"import React, { useContext, useEffect, useState } from 'react';\nimport { observer } from 'mobx-react-lite';\nimport { Image } from 'react-bootstrap';\nimport { useParams } from 'react-router-dom';\n\nimport { Context } from '..';\nimport { getDevice } from '../http/deviceApi';\nimport { AxiosRequestConfig } from 'axios';\n\nconst Device = observer(() => {\n  const { id } = useParams();\n\n  const [device, setDevice] = useState<any>({});\n\n  useEffect(() => {\n    id &&\n      getDevice(id as AxiosRequestConfig<number>).then((data) =>\n        setDevice(data),\n      );\n  }, []);\n  getDevice;\n  return (\n    <div>\n      <Image\n        width={300}\n        height={300}\n        src={process.env.REACT_APP_HOST_URL + device.img}\n      />\n    </div>\n  );\n});\nexport default Device;\n","/Users/apple/Documents/work/2022/fullstack-shop/client/src/pages/shop.tsx",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/store/userStore.ts",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/store/deviceStore.ts",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/components/NavBar/NavBar.tsx",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/components/TypeBar/TypeBar.tsx",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/components/BrandBar/BrandBar.tsx",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/components/DeviceList/DeviceList.tsx",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/components/DeviceItem/DeviceItem.tsx",["174","175"],[],"import React from 'react';\nimport { Card, Col, Image, Button } from 'react-bootstrap';\nimport { useNavigate } from 'react-router-dom';\n\nimport { DEVICE_ROUTE } from '../../utils/constants';\n\nconst DeviceItem = ({ device }: any) => {\n  const navigate = useNavigate();\n\n  return (\n    <Col\n      style={{\n        marginBottom: '30px',\n      }}\n    >\n      <Card\n        style={{\n          width: '18rem',\n          cursor: 'pointer',\n          padding: '0',\n        }}\n        onClick={() => navigate(DEVICE_ROUTE + '/' + device.id)}\n        border=\"light\"\n        bg=\"light\"\n      >\n        <Card.Img\n          variant=\"top\"\n          src={process.env.REACT_APP_HOST_URL + device.img}\n          style={{ width: '100%', height: '250px', objectFit: 'cover' }}\n        />\n        <Card.Body>\n          <Card.Title>{device.name}</Card.Title>\n          <Card.Text>Users Rating {device.rating}</Card.Text>\n        </Card.Body>\n      </Card>\n    </Col>\n  );\n};\n\nexport default DeviceItem;\n","/Users/apple/Documents/work/2022/fullstack-shop/client/src/http/userApi.ts",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/http/index.ts",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/http/deviceApi.ts",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/components/Admin/AdminBrandBar/AdminBrandBar.tsx",["176","177","178","179","180","181","182","183","184","185"],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/components/Admin/ModalWrapper/ModalWrapper.tsx",["186"],[],"import React from 'react';\nimport { Modal, Button } from 'react-bootstrap';\n\ninterface IProps {\n  show: boolean;\n  handleShow: () => void;\n  name: string;\n  children: React.ReactNode;\n}\n\nconst ModalWrapper = ({ show, handleShow, children, name }: IProps) => {\n  return (\n    <div>\n      <Modal show={show} onHide={handleShow}>\n        <Modal.Header closeButton>\n          <Modal.Title>{name}</Modal.Title>\n        </Modal.Header>\n        {children}\n      </Modal>\n    </div>\n  );\n};\n\nexport default ModalWrapper;\n","/Users/apple/Documents/work/2022/fullstack-shop/client/src/components/Admin/CreateButton/CreateButton.tsx",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/assets/icons/Delete.tsx",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/assets/icons/Edit.tsx",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/components/Admin/AdminDeviceList/AdminDeviceList.tsx",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/components/Admin/AdminDeviceItem/AdminDeviceItem.tsx",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/components/Admin/AdminTypeBar/AdminTypeBar.tsx",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/components/Admin/Modals/CreateModals/CreateTypeModal/CreateTypeModal.tsx",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/components/Admin/Modals/CreateModals/CreateDeviceModal/CreateDeviceModal.tsx",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/components/Admin/Modals/CreateModals/CreateBrandModal/CreateBrandModal.tsx",[],[],"/Users/apple/Documents/work/2022/fullstack-shop/client/src/components/Admin/Modals/EditModals/EditBrandOrTypeModal/EditBrandOrTypeModal.tsx",["187"],[],"import { observer } from 'mobx-react-lite';\nimport React, { useContext, useEffect, useLayoutEffect, useState } from 'react';\nimport { Modal, Button, Form } from 'react-bootstrap';\nimport { Context } from '../../../../..';\nimport { editBrand, editType } from '../../../../../http/deviceApi';\nimport ModalWrapper from '../../../ModalWrapper/ModalWrapper';\n\ninterface IProps {\n  show: boolean;\n  handleShow: () => void;\n  name: string;\n}\n\nconst EditBrandOrTypeModal = observer(({ show, handleShow, name }: IProps) => {\n  const { device } = useContext(Context);\n\n  const [value, setValue] = useState<string>('');\n\n  useEffect(() => {\n    setValue(\n      name === 'type' ? device.selectedType.name : device.selectedBrand.name,\n    );\n  }, [show]);\n\n  const edit = (name: string) => {\n    if (name === 'brand') {\n      editBrand(device.selectedBrand.id, { name: value }).then(() =>\n        setValue(''),\n      );\n    }\n    if (name === 'type') {\n      editType(device.selectedType.id, { name: value }).then(() =>\n        setValue(''),\n      );\n    }\n    handleShow();\n  };\n\n  return (\n    <ModalWrapper show={show} handleShow={handleShow} name={`Edit ${name}`}>\n      <Modal.Body>\n        <Form>\n          <Form.Group className=\"mb-3\">\n            <Form.Control\n              value={value}\n              onChange={(event: React.ChangeEvent<HTMLInputElement>) =>\n                setValue(event.target.value)\n              }\n              placeholder={`Type new ${name}`}\n            />\n          </Form.Group>\n          <div className=\"d-flex justify-content-between\">\n            <Button onClick={() => edit(name)} variant=\"primary\" type=\"submit\">\n              Submit\n            </Button>\n            <Button variant=\"secondary\" onClick={handleShow}>\n              Close\n            </Button>\n          </div>\n        </Form>\n      </Modal.Body>\n    </ModalWrapper>\n  );\n});\n\nexport default EditBrandOrTypeModal;\n","/Users/apple/Documents/work/2022/fullstack-shop/client/src/components/Admin/Modals/EditModals/EditDeviceModal/EditDeviceModal.tsx",[],[],{"ruleId":"188","severity":1,"message":"189","line":2,"column":40,"nodeType":"190","messageId":"191","endLine":2,"endColumn":46},{"ruleId":"188","severity":1,"message":"192","line":8,"column":3,"nodeType":"190","messageId":"191","endLine":8,"endColumn":9},{"r